    border-radius: 8px;
    padding: 6px 12px;
    border: 2px solid #0056b3;
    transition: background 0.3s, transform 0.2s;
}

/* Hover effect */
QPushButton#button_detail:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #0056b3, stop:1 #004494);
    transform: scale(1.05);
}

/* Saat ditekan */
