       <property name="styleSheet">
        <string notr="true">QPushButton#button_detail {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #007bff, stop:1 #0056b3);
    color: white;
    font-size: 12px;
