         <string notr="true">/* ==================== QTableWidget ==================== */
QTableWidget {
    background-color: #ffffff; /* Background putih */
    border-radius: 8px; /* Sudut membulat */
    gridline-color: #d1d1d1; /* Warna garis antar sel */
    font-size: 14px;
    selection-background-color: #8dc4ff; /* Warna saat dipilih */
    selection-color: white; /* Warna teks saat dipilih */
    alternate-background-color: #f2f2f2; /* Warna baris selang-seling */
}

/* ==================== Header (Kolom Atas) ==================== */
QHeaderView::section {
    background-color:rgb(202, 202, 202);
    color: black;
    padding: 10px;
    border: none;
    font-weight: bold;
    text-align: center;
    border-bottom: 1px solid #004085; /* Garis bawah tegas */
    font-size: 12px;
}

/* ==================== Baris (Row) ==================== */
QTableWidget::item {
    padding: 8px;
    border-bottom: 1px solid #d1d1d1;
}

/* Alternatif warna tiap baris */
QTableWidget::item:alternate {
    background-color: #f2f2f2; /* Abu muda */
}

/* Hover efek saat kursor di atas baris */
QTableWidget::item:hover {
    background-color: #cce5ff;
    color: black;
    transition: background-color 0.3s ease-in-out;
}

/* ==================== Scrollbar (Gulir) ==================== */
QScrollBar:vertical {
    border: none;
    background: #f1f1f1;
    width: 12px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #007bff;
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    background: none;
}

/* ==================== Sudut (Corner) ==================== */
QTableCornerButton::section {
    background-color: #007bff;
    border: none;
}
QTableWidget {
    gridline-color: #AAAAAA; /* Warna garis grid */
}

QTableWidget::item {
    border-bottom: 1px solid #CCCCCC; /* Garis horizontal */
    border-right: 1px solid #CCCCCC; /* Garis vertikal */
}

